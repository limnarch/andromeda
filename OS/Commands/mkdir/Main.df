#include "<df>/dragonfruit.h"

#include "<ll>/OSDLL/OS.h"

fn Main { ... -- ret }
	0 ret!

	auto i
	1 i!

	while (i@ argc@ <)
		[i@]argv@ MkDir

		1 i +=
	end
end

fn MkDir { path -- }
	auto ok
	auto handle

	511 // permissions
	OSFILEFLAG_MUSTCREATE // flags
	0 // access
	path@ // path
	OSDirectoryCreate ok! handle!

	if (ok@)
		ok@ OSStatusGetName path@ "mkdir: %s: %s\n" OSError
	end

	handle@ OSClose drop
end